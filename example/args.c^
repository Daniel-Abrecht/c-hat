void test(int x[n][m], int n, int m);

void test(int x[n][m], int n, int m){
  int printf(const char *format, ...);
  for(int j=0; j<n; j++){
    for(int i=0; i<m; i++)
      printf("%d ", x[j][i]);
    printf("\n");
  }
}

